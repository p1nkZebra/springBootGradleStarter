<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"

        logicalFilePath="db/changelog/1.0.x/JIRA_task_1/_.xml"

        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog
            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="JIRA_task_1-1" author="p1nkZebra">
        <createTable tableName="google_auth_user">
            <column name="ID" type="BIGINT" autoIncrement="true"  >
                <constraints
                        primaryKey="true"
                        nullable="false"/>
            </column>
            <column name="NAME" type="VARCHAR(200)">
                <constraints nullable="false"/>
            </column>
            <column name="userpic" type="VARCHAR(200)">
                <constraints nullable="false"/>
            </column>
            <column name="email" type="VARCHAR(200)">
                <constraints nullable="false"/>
            </column>
            <column name="gender" type="VARCHAR(50)"/>
            <column name="locale" type="VARCHAR(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>



</databaseChangeLog>
